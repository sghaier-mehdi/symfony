<form method="post" action="{{ path('app_coupon_delete', {'id': coupon.id}) }}" onsubmit="return confirm('Are you sure you want to delete this coupon?');" class="d-inline">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ coupon.id) }}">
    <button class="btn btn-danger">
        <i class="fa fa-trash"></i> Delete
    </button>
</form> 