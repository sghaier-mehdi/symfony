                    --bright-green: #90ee90;
                    --soft-green: #f8faf9;
                    --shadow-green: rgba(144, 238, 144, 0.2);
                }

                /* Add or update in front/base.html.twig <style> or assets/css/style.css */
                .bg-gradient-light-green { background: linear-gradient(135deg, var(--light-green), var(--soft-green)) !important; }
                .bg-light-green { background-color: var(--light-green) !important; }
                .text-green { color: var(--bright-green) !important; }

                .btn-green {
                    background-color: var(--bright-green) !important;
                    border-color: var(--bright-green) !important;
                    color: white !important;
                    transition: background-color 0.5s ease, transform 0.5s ease, box-shadow 0.5s ease;
                }
                .btn-green:hover {
                    background-color: var(--medium-green) !important;
                    transform: translateY(-3px); /* Slight adjustment for effect */
                    box-shadow: 0 8px 20px var(--shadow-green); /* Slight adjustment for effect */
                }

                .btn-outline-green {
                    border-color: var(--bright-green) !important;
                    color: var(--bright-green) !important;
                    transition: background-color 0.5s ease, color 0.5s ease, transform 0.5s ease, box-shadow 0.5s ease;
                }
                .btn-outline-green:hover {
                    background-color: var(--bright-green) !important;
                    color: white !important;
                    transform: translateY(-3px); /* Slight adjustment for effect */
                    box-shadow: 0 8px 20px var(--shadow-green); /* Slight adjustment for effect */
                }

                .glow { box-shadow: 0 0 15px var(--bright-green), 0 0 30px var(--bright-green) !important; transition: box-shadow 0.5s ease !important; }
                .glow:hover { box-shadow: 0 0 25px var(--bright-green), 0 0 50px var(--bright-green) !important; }
                .text-shadow { text-shadow: 4px 4px 8px rgba(0, 0, 0, 0.2); }
                .shadow-xl { box-shadow: 0 20px 40px rgba(0, 0, 0, 0.15) !important; }
                .shadow-md { box-shadow: 0 10px 25px rgba(144, 238, 144, 0.3) !important; }
